CREATE TABLE subjectEntity (
    _name TEXT NOT NULL PRIMARY KEY
);

getSubjectByName:
SELECT *
FROM subjectEntity
WHERE _name = :name;

getAllSubjects:
SELECT *
FROM subjectEntity;

insertSubject:
INSERT OR REPLACE
INTO subjectEntity
VALUES (?);

deleteSubjectByName:
DELETE FROM subjectEntity
WHERE _name = :name;

deleteAllSubjects:
DELETE FROM subjectEntity;